{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "require-jsdoc": "off",
    "max-len": [
      "error",
      {
        "ignoreTemplateLiterals": true,
        "ignoreComments": true,
        "ignoreStrings": true
      }
    ]
  }
}
